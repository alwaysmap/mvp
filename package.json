{
  "name": "awm-prototype",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm run check && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "pnpm run check && vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run tests/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:docker": "docker compose up -d && pnpm test:integration && pnpm test:e2e && docker compose down",
    "test:all": "pnpm test && pnpm test:docker",
    "export": "tsx scripts/export-map.ts",
    "db:setup": "tsx scripts/db-setup.ts",
    "db:teardown": "tsx scripts/db-teardown.ts",
    "db:reset": "pnpm db:teardown && pnpm db:setup",
    "worker": "tsx scripts/start-worker.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:logs:worker": "docker compose logs -f worker"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/d3": "^7.4.3",
    "@types/d3-geo": "^3.1.0",
    "@types/pg": "^8.15.6",
    "@types/qrcode": "^1.5.6",
    "@types/topojson-specification": "^1.0.5",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.0.0",
    "d3": "^7.9.0",
    "d3-geo": "^3.1.1",
    "d3-geo-projection": "^4.0.0",
    "pg": "^8.16.3",
    "pg-boss": "^12.5.2",
    "puppeteer": "^23.0.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.0",
    "topojson-client": "^3.1.0",
    "versor": "^0.2.0"
  }
}
